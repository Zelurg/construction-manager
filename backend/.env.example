# Настройки базы данных
# Замени 'твой_пароль' на свой пароль от PostgreSQL
DATABASE_URL=postgresql://postgres:твой_пароль@localhost:5432/construction_db

# CORS origins - разрешенные источники для запросов
# Для локальной разработки:
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Для работы в локальной сети добавь IP адрес твоего компьютера:
# Пример: если IP твоего компа 192.168.1.100
# CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://192.168.1.100:5173

# Для разработки можно временно разрешить все источники
# ВНИМАНИЕ: НЕ ИСПОЛЬЗУЙ В ПРОДАКШЕНЕ!
# ALLOW_ALL_ORIGINS=true
